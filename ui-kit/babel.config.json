{
    "presets": [
        "@babel/react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "extensions": [
                    ".web.tsx",
                    ".web.ts",
                    ".native.tsx",
                    ".native.ts",
                    ".tsx",
                    ".ts",
                    ".js",
                    ".json"
                ],
                "stripExtensions": [
                    ".web.tsx",
                    ".web.ts",
                    ".native.tsx",
                    ".native.ts"
                ]
            }
        ]
    ]
}